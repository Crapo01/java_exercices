package com.capus.pokemon_exercise;

public class EarthPokemon extends Pokemon{
	
	protected String type;	
	protected String weakness;
	public EarthPokemon(String name, int attack, int defence, int health, String type, String weakness) {
		super(name, attack, defence, health);
		this.type = type;
		this.weakness = weakness;
	}
	
	
	
	
}
